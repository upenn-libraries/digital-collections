<%# Override from Blacklight 9.0.0beta1@de5ddb to render each element in a multivalued field in a single dd %>

<%= render(@layout.new) do |component| %>
  <% component.with_label do %>
    <%= label %>
  <% end %>
  <%  render_field.each do |value| %>
    <% component.with_value do %>
      <%= value %>
    <% end %>
  <% end %>
<% end %>